{
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook",
    "*.pdf": "default"
  },
  "terminal.integrated.tabs.enabled": true,
  "terminal.integrated.profiles.windows": {
    "PowerShell": {
      "source": "PowerShell",
      "icon": "terminal-powershell",
      "args": [
        "-NoLogo"
      ]
    },
    "Command Prompt": {
      "path": [
        "${env:windir}\\Sysnative\\cmd.exe",
        "${env:windir}\\System32\\cmd.exe"
      ],
      "args": [],
      "icon": "terminal-cmd"
    },
    "Git Bash": {
      "source": "Git Bash"
    },
    "Ubuntu-22.04 (WSL)": {
      "path": "C:\\Windows\\System32\\wsl.exe",
      "args": [
        "-d",
        "Ubuntu-22.04"
      ]
    }
  },
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "terminal.integrated.defaultProfile.linux": "zsh",
  "terminal.integrated.shellIntegration.enabled": true,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[csharp]": {
    "editor.defaultFormatter": "ms-dotnettools.csharp"
  },
  "security.workspace.trust.untrustedFiles": "open",
  "code-runner.runInTerminal": true,
  "explorer.confirmDelete": false,
  "prettier.printWidth": 160,
  "git.decorations.enabled": false,
  "editor.parameterHints.enabled": false,
  "python.defaultInterpreterPath": "C:\\Python311\\python.exe",
  "editor.bracketPairColorization.enabled": true,
  "workbench.iconTheme": "vscode-icons",
  "vsicons.dontShowNewVersionMessage": true,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "jest.autoRun": "off",
  "jest.enableCodeLens": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "prettier.singleQuote": true,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "telemetry.telemetryLevel": "off",
  "extensions.autoUpdate": "onlyEnabledExtensions",
  "prettier.semi": false,
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "search.exclude": {
    "**/bower_components": true,
    "**/env": true,
    "**/node_modules/**": true,
    "**/package-lock.json": true,
    "**/venv": true,
    "**/allure-results":true,
    "**/test_results":true,
  },

  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/env/**": true,
    "**/venv/**": true,
    "env-*": true
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "javascript.preferences.jsxAttributeCompletionStyle": "none",
  "typescript.preferences.jsxAttributeCompletionStyle": "none",
  "todo-tree.tree.scanAtStartup": false,
  "todo-tree.tree.autoRefresh": false,
  "todo-tree.highlights.useColourScheme": true,
  "terminal.integrated.fontSize": 12,
  "editor.inlineSuggest.enabled": true,
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": true,
    "scminput": false,
    "yaml": true,
    "javascript": true,
    "javascriptreact": false,
    "jsonc": false,
    "vue": true,
    "json": false,
    "typescriptreact": true,
    "css": false,
    "typescript": true,
    "html": false,
    "python": "true"
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  
  "prettier.arrowParens": "avoid",
  "editor.minimap.renderCharacters": false,
  "editor.minimap.maxColumn": 80,
  "editor.overviewRulerBorder": false,
  "workbench.colorCustomizations": {
    "minimap.background": "#1e1e1e33",
    "statusBarItem.remoteBackground": "#ffffff1a",
    "statusBarItem.remoteForeground": "#cccccc",
    "list.errorForeground": "#f44747",
    "list.hoverBackground": "#ffffff1a",
    "list.warningForeground": "#cca700",
    "tab.hoverBackground": "#ffffff1a",
    "[gruvboxConcoctis dark soft]": {
      "gitDecoration.ignoredResourceForeground": "#6d6d6d",
      "sideBar.foreground": "#a5a5a5",
      "editorError.foreground": "#f44747",
    }
  },
  "editor.renderWhitespace": "none",
  "editor.tokenColorCustomizations": {
    "[Darcula]": {
      "textMateRules": [
        {
          "name": "component",
          "scope": [
            "entity.name.tag.js.jsx support.class.component.js.jsx",
            "entity.name.tag support.class.component",
          ],
          "settings": {
            "foreground": "#2E933C"
          }
        },
        {
          "scope": "entity.name.type.class",
          "settings": {
            "foreground": "#7DAEA3"
          }
        },
        {
          "name": "classes",
          "scope": [
            "support.class.component"
          ],
          "settings": {
            "foreground": "#2E933C",
          }
        },
        {
          "name": "interpolated variable c#",
          "scope": ["variable,meta.interpolation.cs"],
          "settings": {
            "foreground": "#CCCCCC",
          }
        }
      ],
    },
    "[gruvboxConcoctis dark soft]": {
      "textMateRules": [
        {
          "scope": "entity.name.type.class",
          "settings": {
            "foreground": "#7DAEA3"
          }
        },
      ]
    }
  },
  
  "editor.semanticHighlighting.enabled": true,
  "editor.selectionClipboard": false,
  "emmet.showExpandedAbbreviation": "inMarkupAndStylesheetFilesOnly",
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.minimap.autohide": true,
  "editor.minimap.enabled": false,
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.codeLens.enabled": false,
  "gitlens.hovers.enabled": false,
  "prettier.requireConfig": true,
  "editor.suggest.showKeywords": false,
  "editor.acceptSuggestionOnCommitCharacter": false,
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "jestrunner.jestCommand": "npm run test --",
  "prettier.documentSelectors": [
    "[\"**/*.astro\"]"
  ],
  "[astro]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "gitlens.showWelcomeOnInstall": false,
  "editor.accessibilitySupport": "off",
  "cSpell.language": "en,ru",
  "prettier.bracketSameLine": true,
  "volar.inlayHints.eventArgumentInInlineHandlers": false,
  "explorer.sortOrderLexicographicOptions": "upper",
  "volar.codeLens.references": false,
  // proper syntax highlighting
  "gopls": {
    "ui.semanticTokens": true,
    "ui.noSemanticString": true, // delegates string syntax highlighting to vscode
    "ui.noSemanticNumber": true, // delegates number syntax highlighting to vscode
  },
  "svelte.enable-ts-plugin": true,
  "[svelte]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.editor.labelFormat": "short",
  "workbench.tree.indent": 12,
  "explorer.compactFolders": false,
  "go.toolsManagement.autoUpdate": true,
  "window.titleBarStyle": "custom",
  "window.customMenuBarAltFocus": false,
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "dotnet.codeLens.enableReferencesCodeLens": false,
  "editor.fontFamily": "\"JetBrains Mono\"",
  "code-runner.executorMap": {
    "javascript": "node",
    "java": "cd $dir && javac $fileName && java $fileNameWithoutExt",
    "c": "cd $dir && gcc $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "zig": "zig run",
    "cpp": "cd $dir && g++ $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "objective-c": "cd $dir && gcc -framework Cocoa $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "php": "php",
    "python": "python3 -u",
    "perl": "perl",
    "perl6": "perl6",
    "ruby": "ruby",
    "go": "go run",
    "lua": "lua",
    "groovy": "groovy",
    "powershell": "powershell -ExecutionPolicy ByPass -File",
    "bat": "cmd /c",
    "shellscript": "bash",
    "fsharp": "fsi",
    "csharp": "scriptcs",
    "vbscript": "cscript //Nologo",
    "typescript": "ts-node",
    "coffeescript": "coffee",
    "scala": "scala",
    "swift": "swift",
    "julia": "julia",
    "crystal": "crystal",
    "ocaml": "ocaml",
    "r": "Rscript",
    "applescript": "osascript",
    "clojure": "lein exec",
    "haxe": "haxe --cwd $dirWithoutTrailingSlash --run $fileNameWithoutExt",
    "rust": "cargo run $fileName",
    "racket": "racket",
    "scheme": "csi -script",
    "ahk": "autohotkey",
    "autoit": "autoit3",
    "dart": "dart",
    "pascal": "cd $dir && fpc $fileName && $dir$fileNameWithoutExt",
    "d": "cd $dir && dmd $fileName && $dir$fileNameWithoutExt",
    "haskell": "runghc",
    "nim": "nim compile --verbosity:0 --hints:off --run",
    "lisp": "sbcl --script",
    "kit": "kitc --run",
    "v": "v run",
    "sass": "sass --style expanded",
    "scss": "scss --style expanded",
    "less": "cd $dir && lessc $fileName $fileNameWithoutExt.css",
    "FortranFreeForm": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "fortran-modern": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "fortran_fixed-form": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "fortran": "cd $dir && gfortran $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
    "sml": "cd $dir && sml $fileName"
  },
  "terminal.integrated.cursorStyle": "line",
  "files.autoSave": "afterDelay",
  "explorer.confirmDragAndDrop": false,
  "python.analysis.autoImportCompletions": true,
  "python.analysis.indexing": true,
  "python.analysis.packageIndexDepths": [
      { "name": "", "depth": 2, "includeAllSymbols": true }],
  "omnisharp.organizeImportsOnFormat": true,
  "workbench.editor.empty.hint": "hidden",
  "editor.fontLigatures": false,
  "editor.linkedEditing": true,
  "git.confirmSync": false,
  "editor.unicodeHighlight.invisibleCharacters": false,
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "extensions.ignoreRecommendations": true,
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "editor.codeActionsOnSave": {
    // "source.organizeImports": "always",
    "source.unusedImports": "always",
    "source.fixAll.eslint": "never",
    "source.fixAll.stylelint": "always"
  },
  // "css.validate": false,
  // "less.validate": false,
  // "scss.validate": false,
  // "stylelint.validate": ["css", "scss"],
  "eslint.validate": ["javascript"],
  "editor.formatOnSave": true,
  "editor.codeLensFontFamily": "\"JetBrains Mono\"",
  "editor.inlayHints.fontFamily": "\"JetBrains Mono\"",
  "editor.inlineSuggest.fontFamily": "\"JetBrains Mono\"",
  "dotnet.projects.enableAutomaticRestore": false,
  "window.menuBarVisibility": "toggle",
  "rust-analyzer.inlayHints.chainingHints.enable": false,
  "rust-analyzer.inlayHints.parameterHints.enable": false,
  "rust-analyzer.inlayHints.typeHints.enable": false,
  "rust-analyzer.inlayHints.renderColons": false,
  "rust-analyzer.inlayHints.closingBraceHints.enable": false,
  "rust-analyzer.check.command": "check",
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer",
    "editor.formatOnSave": true
  },

  "editor.semanticTokenColorCustomizations": {
    "enabled": true,
    "rules": {
        "*.mutable": {
            "underline": false, /* Rust mutable undeerline highlight */
        }
    },
    "[gruvboxConcoctis dark soft]": {
      "enabled": true,
      "rules": {
        "variable.mutable": "#D4BE98"
      }
    }
},
"diffEditor.hideUnchangedRegions.enabled": true,
"gitlab.aiAssistedCodeSuggestions.enabled": false,
"debug.allowBreakpointsEverywhere": true,
"rust-analyzer.lens.implementations.enable": false,
"rust-analyzer.hover.actions.references.enable": true,
"git.openRepositoryInParentFolders": "always",
"github.copilot.editor.enableAutoCompletions": false, // fix for copilot on press
"github.copilot.editor.enableCodeActions": false,
"http.proxySupport": "on",
"http.proxyStrictSSL": false,
"http.proxy": "",
"terminal.integrated.fontFamily": "FiraCode Nerd Font",
"workbench.layoutControl.enabled": false,
"gruvboxConcoctis.useItalicFont": false,
"diffEditor.ignoreTrimWhitespace": false,
"codesnap.showWindowControls": false,
"codesnap.containerPadding": "0",
"scm.showHistoryGraph": false,
"vue.autoInsert.parentheses": false,
"vue.codeLens.enabled": false,
"haskell.manageHLS": "GHCup",
"workbench.colorTheme": "gruvboxConcoctis dark soft",
}
